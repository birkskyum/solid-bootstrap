{
  "name": "solid-bootstrap",
  "version": "1.0.7",
  "description": "Bootstrap 5 components built with SolidJS",
  "homepage": "https://solid-libs.github.io/solid-bootstrap",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/solid-libs/solid-bootstrap.git#master"
  },
  "author": "Brendan O'Reilly",
  "license": "MIT",
  "type": "module",
  "main": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/esm/index.d.ts",
      "solid": "./dist/esm/index.jsx",
      "default": "./dist/esm/index.js"
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "tsc && rollup -c && node dist/esm/prepublish.js",
    "prepublishOnly": "npm run build",
    "buildpackage": "cd dist && npm pack",
    "watch": "tsc -noemit -watch"
  },
  "dependencies": {
    "dom-helpers": "^5.2.0",
    "solid-bootstrap-core": "^1.0.6",
    "solid-react-transition": "^1.0.5"
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@babel/preset-typescript": "^7.16.7",
    "@rollup/plugin-babel": "5.3.0",
    "@rollup/plugin-node-resolve": "13.1.3",
    "babel-preset-solid": "^1.3.6",
    "rollup": "^2.67.3",
    "rollup-plugin-terser": "^7.0.2",
    "solid-js": ">=1.2.5",
    "typescript": "^4.5.5"
  },
  "peerDependencies": {
    "solid-js": ">=1.2.5"
  }
}
